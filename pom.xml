<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">    <modelVersion>4.0.0</modelVersion>    <groupId>com.comptel.bytelalarmhandler.clearer</groupId>    <artifactId>ByTel_Alarm_Clearer</artifactId>    <version>1.0.0-PRE3</version>    <name>BYTEL_ALARM_CLEARER Node</name>    <dependencies>        <dependency>            <groupId>junit</groupId>            <artifactId>junit</artifactId>            <version>4.8.2</version>            <scope>test</scope>        </dependency>        <dependency>            <groupId>com.comptel.mc</groupId>            <artifactId>mcp-javanode-api</artifactId>            <version>2.3.0</version>            <scope>provided</scope>        </dependency>        <dependency>            <groupId>com.comptel.mc</groupId>            <artifactId>mcp-javanode-impl</artifactId>            <version>1.0.0</version>            <scope>provided</scope>        </dependency>        <dependency>            <groupId>org.mockito</groupId>            <artifactId>mockito-core</artifactId>            <version>1.7</version>            <scope>test</scope>        </dependency>        <dependency>            <groupId>com.comptel.mc</groupId>            <artifactId>mcp-javanode-mocks</artifactId>            <version>1.0.0</version>            <scope>test</scope>        </dependency>        <dependency>            <groupId>com.comptel.event.installer.ng</groupId>            <artifactId>eventmanagement-engine-solution</artifactId>            <version>2013.3</version>        </dependency>        <dependency>            <groupId>com.google.code.gson</groupId>            <artifactId>gson</artifactId>            <version>2.8.2</version>        </dependency>        <!-- For reading .properties file -->        <dependency>            <groupId>commons-configuration</groupId>            <artifactId>commons-configuration</artifactId>            <version>1.10</version>        </dependency>        <!-- Oracle -->        <dependency>            <groupId>c3p0</groupId>            <artifactId>c3p0</artifactId>            <version>0.9.1.2</version>        </dependency>        <dependency>            <groupId>com.oracle</groupId>            <artifactId>ojdbc6</artifactId>            <version>11.2.0.2.0</version>        </dependency>    </dependencies>    <build>        <plugins>            <plugin>                <groupId>org.apache.maven.plugins</groupId>                <artifactId>maven-dependency-plugin</artifactId>                <version>2.3</version>                <executions>                    <execution>                        <id>copy-dependencies</id>                        <phase>package</phase>                        <goals>                            <goal>copy-dependencies</goal>                        </goals>                        <configuration>                            <outputDirectory>${project.build.directory}/dependencies</outputDirectory>                            <overWriteReleases>false</overWriteReleases>                            <overWriteSnapshots>false</overWriteSnapshots>                            <overWriteIfNewer>true</overWriteIfNewer>                            <includeScope>runtime</includeScope>                        </configuration>                    </execution>                </executions>            </plugin>            <plugin>                <groupId>org.apache.maven.plugins</groupId>                <artifactId>maven-shade-plugin</artifactId>                <version>2.2</version>                <executions>                    <execution>                        <phase>package</phase>                        <goals>                            <goal>shade</goal>                        </goals>                        <!-- Just for testing WS on Kcell Test machine as no maven is installed -->                        <configuration>                            <transformers>                                <transformer implementation="org.apache.maven.plugins.shade.resource.ManifestResourceTransformer">                                    <mainClass>com.comptel.cc.event.EM10Generator</mainClass>                                </transformer>                            </transformers>                        </configuration>                        <!-- Just for testing WS on Kcell Test machine as no maven is installed -->                    </execution>                </executions>            </plugin>            <plugin>                <groupId>org.apache.maven.plugins</groupId>                <artifactId>maven-compiler-plugin</artifactId>                <version>3.1</version>                <configuration>                    <source>1.8</source>                    <target>1.8</target>                </configuration>            </plugin>            <plugin>                <groupId>org.apache.maven.plugins</groupId>                <artifactId>maven-jar-plugin</artifactId>                <version>2.2</version>                <configuration>                    <finalName>app_AdapterNode</finalName>                </configuration>            </plugin>            <plugin>                <groupId>com.comptel.eventlink</groupId>                <artifactId>eltools-plugin</artifactId>                <version>1.5-SNAPSHOT</version>                <executions>                    <execution>                        <id>cfg</id>                        <phase>package</phase>                        <goals>                            <goal>resources</goal>                            <goal>package</goal>                        </goals>                    </execution>                </executions>                <configuration>                    <autoversion>true</autoversion>                    <packageName>BYTEL_ALARM_CLEARER.el</packageName>                </configuration>            </plugin>            <!-- With the configuration for surefire given below, tests uses logging.properties -->            <plugin>                <artifactId>maven-surefire-plugin</artifactId>                <version>2.16</version>                <configuration>                    <systemProperties>                        <property>                            <name>java.util.logging.config.file</name>                            <value>src/test/resources/logging.properties</value>                        </property>                    </systemProperties>                </configuration>            </plugin>        </plugins>    </build></project>